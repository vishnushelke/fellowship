
package com.bridgelabz.data_structure;

public class Stack<T> {

	public Object []stack=null;
	public int top;
	public void push(T data)
	{
		stack[top]=data;
		top++;
	}
	public void pop()
	{
		T data;
		data=(T) stack[top];
		stack[top]=0;
		top--;
		System.out.println("Popped Element is : "+data);
	}
	public void peek()
	{
		T data;
		data=(T)stack[top-1];
		System.out.println("Peeked Element is : "+data);
	}
}
